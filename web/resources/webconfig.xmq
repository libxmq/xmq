webserver {
    user = tinyw
    error_log(filter = notice) = /var/log/error.log
    pid = /var/run/webserver.pid

    http {
        default_type = application/octet-stream
        access_log(filter = main) = /var/log/access.log

        sendfile = on
        // tcp_nopush = on

        keepalive_timeout = 65
        gzip = on
        server_tokens = off

        include = /etc/webserver/conf.d/*.conf
    }
}
