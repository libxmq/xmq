parse = line++-#a, -#a?.
-line = xmq|xmqd|xmqa|ix|not|gr|ru|lh|pa|trb|trp|trt|tra|trpp|st|scan|core|crea|step|testnot|block|ignore|ignorec.

-xmq = -'xmq: ', -~[#a]+.
-xmqd = -'xmqd: ', -~[#a]+.
-xmqa = -'xmqa: ', -~[#a]+.
-ix = -'(ixml) ', -~[#a]+.
-not = -'(ixml.pa) not', -~[#a]+.
-gr = -'(ixml.gr) ', -~[#a]+.
-ru = -'(ixml.rules) ', -~[#a]+.
-lh = -'(ixml.lh) ', -~[#a]+.
-st = -'(ixml.st) ', -~[#a]+.
-pa    = -'(ixml.pa) parsing', -~[#a]+.
scan  = -'(ixml.pa) input[',index,-"]=",char.
-core = -'(ixml.pa) state', -~[#a]+.
-crea = -'(ixml.pa) crea ', -~[#a]+.
step  = -'(ixml.pa) step @',toki,-' ', iinfo, ' (', id, -')', s, name, s, -'→', s, body?, dot?, where, cost?, -' ', info, -~[#a]*.
-ignorec  = -'(ixml.pa) @',-[N]+,-' created ', -~[#a]*.
-testnot  = -'(ixml.pa) @',-[N]+,-' test ', -~[#a]*.
-ignore = -'(ixml.pa) (', -~[#a]*.
block = -'(ixml.pa) blocked', btype, -' (', id, -')', -~[#a]*.
toki  = [N]+.
btype = [N]+.
info = -'{', ~['}']+, -'}'.
iinfo = [Ll;N;'_.']+.

-trb = -'(ixml.tr) building ', -~[#a]+.
-trp = -'(ixml.tr) processing ', -~[#a]+.
-trt = -'(ixml.tr) trying ', -~[#a]+.
tra = -'(ixml.tr) adding (',id,-')', -~[#a]+.
-trpp = -'(ixml.tr) popping ', -~[#a]+.

char = -"'",~[#a],-"'" | 'ω' | '#',[N;L]+.
token = -"=", ~[#a]*.

index = [N]+.
id = [N;',-']+.
name = [L;N;'|[]_#-$;+!~']+.
-s = -' '+.
body = ~[' ·'], ~[#a;'·']+.
dot = '·', ~[#a]+.
-where = type, -'[', from, -'-', to, -'/', total, -']'.
cost = '<'+.
type = "complete"|"partial"|"prediction"|"not".
from = [N]+.
to = [N]+.
total = [N]+.
